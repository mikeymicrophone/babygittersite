<h1>New version</h1>

<% form_for(@version) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :maintainer %><br />
    <%= f.text_field :maintainer %>
  </p>
  <p>
    <%= f.label :repo_path %><br />
    <%= f.text_field :repo_path %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', versions_path %>
